<!-- Modal-->
<div class="modal fade" id="addMachineTypeModal" tabindex="-1" role="dialog" aria-labelledby="MachineTypeModalLabel">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h4 class="modal-title" id="MachineTypeModalLabel">Add New Machine Type</h4>
                <button type="button" class="close" data-bs-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
            </div>
            <div class="modal-body">
                @{
                    var m = new haver.Models.MachineType();
                    <partial name="_MachineTypeCreate" model="m" />
                }
            </div>
        </div>
    </div>
</div>