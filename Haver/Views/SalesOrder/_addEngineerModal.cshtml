<!-- Modal-->
<div class="modal fade" id="addEngineerModal" tabindex="-1" role="dialog" aria-labelledby="EngineermyModalLabel">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h4 class="modal-title" id="EngineermyModalLabel">Add New Engineer</h4>
                <button type="button" class="close" data-bs-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
            </div>
            <div class="modal-body">
                @{
                    var s = new haver.Models.Engineer();
                    <partial name="_EngineerCreate" model="s" />
                }
            </div>
        </div>
    </div>
</div>